{
    "restructuredtext.confPath": "",
    "python.testing.unittestEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=F0401", "--disable=C0115"
    ],
    "cmake.cmakePath": "cmake",
    "cmake.buildArgs": [
        "-G",
        "Ninja",
        "-S",
        "${workspaceFolder}",
        "-B",
        "${workspaceFolder}/build"
    ],
    "cmake.generator" : "Ninja",
    "python.testing.unittestArgs": [
        "discover",
        "-v",
        "-s",
        "./generator/pygenerator/test/",
        "-p",
        "*test*.py"
    ],
    "python.analysis.extraPaths" : ["./generator/pygenerator/src"],
    "python.testing.pytestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.cwd": "./generator/pygenerator/src",
    "files.associations": {
        "array": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
